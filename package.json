{
	"name": "gitembed",
	"description": "Create, updates, and removes embeddings of the repository files, using Voyage and Pinecone",
	"scripts": {
		"build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=index.js",
		"run": "node index.js",
		"dev": "esrun src/index.ts",
		"prepare": "npm run build"
	},
	"dependencies": {
		"@pinecone-database/pinecone": "^3.0.0",
		"dotenv": "^16.4.5",
		"voyageai": "0.0.1-2"
	},
	"devDependencies": {
		"esbuild": "^0.23.0",
		"esrun": "^3.2.26",
		"typescript": "^5.5.4",
		"@types/node": "^22.3.0"
	}
}